<div class="panel panel-default">
  <div class="panel-body">
    <% unless movie_show %>
      <h4>Movie: <%= link_to review.movie.title, movie_url(review.movie_id) %></h4>
    <% end %>
    <h4>Email: <%= review.email %></h4>
    <h4>Posted: <%= time_ago_in_words(review.created_at) + ' ago'%></h4>
    <h4>Rating: <%= review.rating %></h4>
    <h4>Comment: <%= review.comment.length > 0 ? review.comment : "none" %></h4>
  </div>
</div>
